    "\n",
    "data = pd.read_csv(\"challenge.txt\", sep='\\t', header=None)\n",
    "\n",
    "rows, cols = data.shape\n",
    "print(f\"Dimensions: {rows} rows × {cols} columns\")\n",
    "\n",
    "missing = data.isnull().sum().sum()\n",
    "\n",
    "if missing > 0:\n",
    "    print(\"no missiong value\")\n",
    "else:\n",
    "    print(\"there is missing values\")"
